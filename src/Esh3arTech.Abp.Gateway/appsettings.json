{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "messages-route": {
        "ClusterId": "messages-cluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "messages-cluster": {
        "LoadBalancingPolicy": "RoundRobin",
        "Destinations": {
          "messages-destination1": {
            "Address": "https://localhost:44306/"
          },
          "messages-destination2": {
            "Address": "https://localhost:44307/"
          }
        }
      }
    }
  },
  "AllowedHosts": "*"
}
