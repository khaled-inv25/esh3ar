@page
@using Esh3arTech.Localization
@using Microsoft.Extensions.Localization
@inject IStringLocalizer<Esh3arTechResource> L
@model Esh3arTech.Web.Pages.Plans.Subscriptions.HistoryModel
@{
}

@section scripts
{
    <abp-script src="/Pages/Plans/Subscriptions/History.js" />
}

<abp-card>
    <abp-card-header>
        <abp-row>
            <abp-column size-md="_8">
                <h2>@L["History"] Esh3arTech Client Name</h2>
            </abp-column>
            <abp-column size-md="_2" class="text-end">
                <abp-button id="ExportToExcel" text="@L["ExportToExcel"].Value" icon="plus" button-type="Primary">
                </abp-button>
            </abp-column>
            <abp-column size-md="_2" class="text-end">
                <abp-button id="ExportToPdf" text="@L["ExportToPdf"].Value" icon="plus" button-type="Primary">
                </abp-button>
            </abp-column>
        </abp-row>
    </abp-card-header>
    <abp-card-body>
        <abp-table striped-rows="true" id="HistoryTable"></abp-table>
    </abp-card-body>
</abp-card>