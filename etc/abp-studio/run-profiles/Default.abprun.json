{
  "applications": {
    "Esh3arTech.Web": {
      "type": "dotnet-project",
      "launchUrl": "https://localhost:44306",
      "path": "../../../src/Esh3arTech.Web/Esh3arTech.Web.csproj",
      "kubernetesService": ".*-web$",
      "healthCheckEndpoint": "/health-status",
      "healthUiEndpoint": "/health-ui",
      "execution": {
        "order": 2
      }
    }
  },
  "containers": {
    "serviceName": "Esh3arTech-Containers",
  },
  "tasks":{
    "Initialize Solution":{
    "type": "cli",
    "runOnSolutionOpen": true,
    "startCommand": "etc/scripts/initialize-solution.ps1",
    "workingDirectory": "../../../"
    },
    "Migrate Database":{
    "type": "cli",
    "runOnSolutionOpen": false,
    "startCommand": "etc/scripts/migrate-database.ps1",
    "workingDirectory": "../../../"
    }
  }
}